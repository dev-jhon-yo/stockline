{
	"name": "roblox-ts-project-template",
	"version": "1.0.0",
	"description": "A Roblox game made with React and Roblox-TS",
	"license": "MIT",
	"author": "Christopher Buss <christopher.buss@pm.me> (https://github.com/christopher-buss)",
	"type": "module",
	"scripts": {
		"assets:upload": "asphalt sync",
		"postassets:upload": "eslint src/shared/assets.d.ts --fix",
		"clean": "npx rimraf out/ && npx rimraf dist",
		"commit": "commit",
		"darklua": "darklua process out dist --verbose",
		"dev:build": "rbxts-build build",
		"dev:compile": "rbxts-build compile",
		"dev:open": "rbxts-build open",
		"dev:restart": "pnpm run dev:stop && pnpm run dev:start",
		"dev:start": "rbxts-build start",
		"dev:stop": "rbxts-build stop",
		"dev:sync": "rbxts-build sync",
		"postdev:sync": "eslint src/types/services.d.ts --fix",
		"dev:watch": "rbxts-build watch",
		"eslint-config": "npx @eslint/config-inspector",
		"lint": "eslint --cache",
		"lint:ci": "eslint --cache --cache-strategy content",
		"prepare": "node .husky/install.js",
		"prod:build": "npm run clean && NODE_ENV=production && npx rbxtsc --verbose && npm run darklua",
		"prod:sync": "rojo serve ./build.project.json"
	},
	"lint-staged": {
		"*": "eslint --fix --max-warnings 0 --no-warn-ignored --cache --cache-strategy content"
	},
	"dependencies": {
		"@flamework/components": "catalog:prod",
		"@flamework/core": "catalog:prod",
		"@flamework/networking": "catalog:prod",
		"@rbxts/flamework-binary-serializer": "catalog:prod",
		"@rbxts/janitor": "catalog:prod",
		"@rbxts/lapis": "catalog:prod",
		"@rbxts/lapis-mockdatastore": "catalog:prod",
		"@rbxts/log": "catalog:prod",
		"@rbxts/luau-polyfill": "catalog:prod",
		"@rbxts/message-templates": "catalog:prod",
		"@rbxts/player-module-types": "catalog:prod",
		"@rbxts/pretty-react-hooks": "catalog:prod",
		"@rbxts/promise-child": "catalog:prod",
		"@rbxts/rbx-better-signal": "catalog:prod",
		"@rbxts/rbx-debug": "catalog:prod",
		"@rbxts/react": "catalog:prod",
		"@rbxts/react-reflex": "catalog:prod",
		"@rbxts/react-roblox": "catalog:prod",
		"@rbxts/reflex": "catalog:prod",
		"@rbxts/remap": "catalog:prod",
		"@rbxts/ripple": "catalog:prod",
		"@rbxts/services": "catalog:prod",
		"@rbxts/sift": "catalog:prod",
		"@rbxts/t": "catalog:prod",
		"@rbxts/ui-labs": "catalog:prod",
		"@rbxts/validate-tree": "catalog:prod"
	},
	"devDependencies": {
		"@antfu/ni": "catalog:script",
		"@commitlint/cli": "catalog:script",
		"@commitlint/config-conventional": "catalog:script",
		"@commitlint/prompt-cli": "catalog:script",
		"@commitlint/types": "catalog:script",
		"@eslint/config-inspector": "catalog:lint",
		"@isentinel/eslint-config": "catalog:lint",
		"@isentinel/tsconfig": "catalog:types",
		"@rbxts/compiler-types": "catalog:types",
		"@rbxts/types": "catalog:types",
		"eslint": "catalog:lint",
		"eslint-plugin-pnpm": "catalog:lint",
		"eslint-plugin-react-hooks": "catalog:lint",
		"eslint-plugin-react-naming-convention": "catalog:lint",
		"eslint-plugin-react-roblox-hooks": "catalog:lint",
		"eslint-plugin-react-x": "catalog:lint",
		"git-pull-run": "catalog:script",
		"husky": "catalog:script",
		"jiti": "catalog:script",
		"lint-staged": "catalog:lint",
		"rbxts-build": "catalog:script",
		"rbxts-transform-debug": "catalog:build",
		"rbxts-transform-env": "catalog:build",
		"rbxts-transformer-flamework": "catalog:build",
		"rimraf": "catalog:script",
		"roblox-ts": "catalog:build",
		"ts-plugin-sort-import-suggestions": "catalog:types",
		"typescript": "catalog:types"
	},
	"packageManager": "pnpm@10.28.2",
	"rbxts-build": {
		"syncLocation": "src/types/services.d.ts",
		"names": {
			"build": "dev:build",
			"compile": "dev:compile",
			"open": "dev:open",
			"watch": "dev:watch"
		}
	}
}
